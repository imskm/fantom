{
	"name": "imskm/fantom",
	"description": "The Fantom framework.",
	"keywords": ["framework", "fantom", "php mvc framework"],
	"authors": [
		{
			"name": "Shek Muktar",
			"email": "skm_im07@live.com"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^6"
	},
	"autoload": {
		"files": [
			"src/Support/helpers.php"
		],
		"psr-4": {
			"Fantom\\": "src/"
		}
	},
	"autoload-dev": {
		"files": [
			"tests/bootstrap.php"
		],
		"psr-4": {
			"Fantom\\Tests\\": "tests/",
			"App\\": "app/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"prefer-stable": true
}
